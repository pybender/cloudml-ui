# .coveragerc to control coverage.py
[run]
branch = True
omit = *config*, *admin.py*, */admin/*, *fixtures.py*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if 0:
